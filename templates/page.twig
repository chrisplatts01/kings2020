{% extends "base.twig" %}

{% block content %}
  <!-- page.twig -->
	<article class="page-content post-type-{{post.post_type}}" id="post-{{post.ID}}">
		<section class="article__wrapper wrapper">
			<h1 class="article-h1">{{post.title}}</h1>
			<div class="article-body">
				{{post.content}}
			</div>
		</section>
	</article>
{% endblock %}
